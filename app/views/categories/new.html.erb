<%= render 'shared/content_title',  title: 'Create a new category' %>
<%= render 'shared/error', obj: @category %>
<div class="well">
  <%= form_for @category do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br/>
    <%= f.submit 'Create Category', class: 'btn btn-primary' %>

  <% end %>
</div>